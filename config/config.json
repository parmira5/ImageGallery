{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "image-gallery-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/imageGallery/ImageGalleryWebPart.js",
          "manifest": "./src/webparts/imageGallery/ImageGalleryWebPart.manifest.json"
        }
      ]
    },
    "image-gallery-form-form-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/imageGalleryForm/ImageGalleryFormFormCustomizer.js",
          "manifest": "./src/extensions/imageGalleryForm/ImageGalleryFormFormCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ImageGalleryWebPartStrings": "lib/webparts/imageGallery/loc/{locale}.js",
    "ImageGalleryFormFormCustomizerStrings": "lib/extensions/imageGalleryForm/loc/{locale}.js"
  }
}
